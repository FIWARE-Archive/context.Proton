            "filterExpression":"(EngineStatistics.engine_rpm_dev/EngineStatistics.engine_rpm_avg) > 0.1 || (EngineStatistics.propeller_pitch_dev/EngineStatistics.propeller_pitch_avg) > 0.1"
          }
        ],
        "computedVariables": [
           {
            "name": "Count",
            "aggregationType": "Count",
            "EngineStatistics": "1"
          }
        ],       
        "evaluationPolicy": "Immediate",
        "cardinalityPolicy": "Single",
        "internalSegmentation": [],
