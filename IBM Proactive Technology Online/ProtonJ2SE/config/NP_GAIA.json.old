    "epas": [
     
        {
        "name": "EngineStatisticsEPA",
        "createdDate": "Tue Dec 22 2015",
        "epaType": "Aggregate",
        "context": "EngineRunning",
        "inputEvents": [
          {
            "name": "SensorReading",            
            "consumptionPolicy": "Consume",
            "instanceSelectionPolicy": "First"
          }
        ],
        "computedVariables": [
           {
            "name": "AverageRPM",
            "aggregationType": "Average",
            "SensorReading": "SensorReading.engine_rpm"
          },
          {
            "name": "AveragePitch",
            "aggregationType": "Average",
            "SensorReading": "SensorReading.propeller_pitch"
          }
        ],       
        "evaluationPolicy": "Deferred",
        "cardinalityPolicy": "Single",
        "internalSegmentation": [],
        "derivedEvents": [
          {
            "name": "EngineStatistics",
            "reportParticipants": false,           
            "expressions": {
              "engine_rpm_dev": "ArrayStandardDev(SensorReading.engine_rpm)",
              "propeller_pitch_dev": "ArrayStandardDev(SensorReading.propeller_pitch)",
               "engine_rpm_avg": "AverageRPM",
              "propeller_pitch_avg": "AveragePitch"
            }
          }
        ]
      },
      
       {
        "name": "EngineSteadyStateEPA",
        "createdDate": "Tue Dec 22 2015",
        "epaType": "Aggregate",
        "context": "EngineCalculateSteadyState",
        "inputEvents": [
          {
            "name": "EngineStatistics",            
            "consumptionPolicy": "Consume",
            "instanceSelectionPolicy": "First",
            "filterExpression":"(EngineStatistics.engine_rpm_dev/EngineStatistics.engine_rpm_avg) <= 0.1 && (EngineStatistics.propeller_pitch_dev/EngineStatistics.propeller_pitch_avg) <= 0.1"
          }
        ],
        "computedVariables": [
           {
            "name": "Count",
            "aggregationType": "Count",
            "EngineStatistics": "1"
          }
        ],       
        "evaluationPolicy": "Immediate",
        "cardinalityPolicy": "Single",
        "internalSegmentation": [],
        "assertion": "Count>=15",
        "derivedEvents": [
          {
            "name": "EngineSteadyState",
            "reportParticipants": false,           
            "expressions": {
            }
          }
        ]
      },
      
      {
        "name": "EngineNotSteadyStateEPA",
